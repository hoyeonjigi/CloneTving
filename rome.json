{
	"$schema": "https://docs.rome.tools/schemas/12.1.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": false
	},
	"javascript": {
		"formatter": {
			"semicolons": "always",
			"quoteStyle": "double",
			"trailingComma": "es5",
			"quoteProperties": "asNeeded"
		}
	},
	"formatter": {
		"enabled": true,
		"formatWithErrors": true,
		"indentStyle": "tab",
		"indentSize": 2,
		"lineWidth": 80,
		"ignore": []
	}
}
